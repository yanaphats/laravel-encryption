{
	"name": "sandbox/laravel-encryption",
	"description": "Auto Encrypt and Decrypt Database through Eloquent",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Sandbox Dev",
			"email": "info@sandbox.co.th"
		}
	],
	"require": {
		"php": "^8.0"
	},
	"autoload": {
		"psr-4": {
			"Sandbox\\DBEncryption\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Sandbox\\DBEncryption\\Tests\\": "tests",
			"Sandbox\\DBEncryption\\Tests\\Database\\Factories\\": "tests/database/factories"
		},
		"files": [
			"src/helpers.php"
		]
	},
	"require-dev": {
		"orchestra/testbench": "^6.0",
		"phpunit/phpunit": "^9.4"
	},
	"extra": {
		"laravel": {
			"providers": [
				"Sandbox\\DBEncryption\\Providers\\DBEncryptionServiceProvider"
			]
		}
	},
	"scripts": {
		"test": "vendor/bin/phpunit",
		"test-f": "vendor/bin/phpunit --filter"
	}
}